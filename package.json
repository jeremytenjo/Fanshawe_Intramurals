    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "element-ui": "^2.10.1",
    "express": "^4.15.2",
    "express-session": "^1.15.1",
    "express-validator": "^6.1.1",
    "formidable": "^1.1.1",
