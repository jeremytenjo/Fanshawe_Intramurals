    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^3.1.4",
    "element-ui": "^1.2.4",
    "express": "^4.15.2",
    "express-session": "^1.15.1",
    "express-validator": "^3.1.2",
    "formidable": "^1.1.1",
    "mongodb": "^2.2.24",
    "mongoose": "^4.8.6",
    "morgan": "^1.8.1",
